{
  "name": "@element-plus/interactive-docs",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@7.12.0",
  "scripts": {
    "dev": "histoire dev",
    "build": "histoire build",
    "preview": "histoire preview",
    "preview:build": "pnpm run build && histoire preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "create": "tsx script/create/index.ts create",
    "up": "taze major -I",
    "postinstall": "npx simple-git-hooks"
  },
  "dependencies": {
    "element-plus": "npm:element-plus@latest",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.4",
    "@histoire/plugin-vue": "0.15.7",
    "@types/inquirer": "^9.0.3",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vueuse/core": "^9.13.0",
    "commander": "^9.5.0",
    "consola": "^2.15.3",
    "eslint": "^8.38.0",
    "histoire": "0.15.7",
    "inquirer": "^9.1.5",
    "lint-staged": "^13.2.1",
    "pathe": "^0.3.9",
    "sass": "^1.61.0",
    "simple-git-hooks": "^2.8.1",
    "taze": "^0.9.1",
    "tsx": "^3.12.6",
    "typescript": "^4.9.5",
    "unplugin-auto-import": "^0.11.5",
    "unplugin-vue-components": "^0.22.12",
    "vite": "^4.2.1",
    "vue-tsc": "^1.2.0"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue,md}": [
      "eslint --fix"
    ]
  }
}
